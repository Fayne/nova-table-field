{
  "name": "outl1ne/nova-table-field",
  "description": "Table field for Laravel Nova",
  "keywords": [
    "laravel",
    "nova",
    "tables",
    "outl1ne"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kaspar Rosin",
      "email": "kaspar.rosin@gmail.com",
      "role": "Developer"
    },
    {
      "name": "outl1ne",
      "email": "info@outl1ne.com",
      "role": "Maintainer"
    },
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/nova": "^5.0",
    "nova-kit/nova-packages-tool": "^2.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Outl1ne\\NovaTableField\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Outl1ne\\NovaTableField\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Outl1ne\\NovaTableField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
